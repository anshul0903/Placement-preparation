#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main()
{
    int a,b,i,n;
    cin>>a;
    vector<int> v1;
    for(i=0;i<a;i++)
    {
        cin>>n;
        v1.push_back(n);
    }
    cin >> b;
    cin>>i>>n;
    v1.erase(v1.begin()+b-1);
    v1.erase(v1.begin()+i-1,v1.begin()+n-1);
    cout<<v1.size()<<endl;
    for(i=0;i<v1.size();i++)
    cout<<v1[i]<<" ";

      return 0;
    }
