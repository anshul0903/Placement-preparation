#include<stdio.h>
#include<string.h>
int main()
{
    char a[100],b[100];
    scanf("%[^\n]%*c",a);
    int j=0,n=strlen(a),i,space=0,k=0;
            while(j<n)
            {
                if(a[j]==32 && space==2)
                break;
                else if(a[j]==32)
                {
                    b[j]=a[j];
                    j++;
                }
                else
                {
                    while(a[j]!=32)
                    {
                        b[j]=a[j];
                        j++;
                    }
                    space++;
                }
            }
            b[j]='\0';
            for(i=j+1;i<n;i++) //since there is space
            {
            printf("%c",a[i]);
            }
            printf(" ");
            printf("%s",b);           
            
    return 0;
}
