#include <stdio.h>
#define max 5
void dfs(int adj[][max],int visited[],int start);
//DFS algorithmm for graphs
int main()
{
    int visited[max]={0};
    int adj[max][max],i,j;
    for(i=0;i<max;i++)
    for(j=0;j<max;j++)
    scanf("%d",&adj[i][j]);
    dfs(adj,visited,0);
    
	return 0;
}

void dfs(int adj[][max],int visited[],int start)
{
    int stack[max],top=-1,i;
    printf("%c-",i+65);
    visited[start]=1;
    stack[++top]=start;
    while(top!=-1)
    {
        start=stack[top];
        for(i=0;i<max;i++)
        {
            if(adj[start][i]==1 && visited[i]==0)
            {
                visited[i]=1;
                stack[++top]=i;
                printf("%c-",i+65);
                break;
            }
        }
        if(i==max)
        top--;
    }
}
