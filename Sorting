#include<stdio.h>
#include<string.h>
int main()
{
    char a[10],b;
    scanf("%s",a);
    int n=strlen(a);
    int i,j;
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            int x=a[i]-'0',y=a[j]-'0';
            if(x>y)
            {
                b=a[i];
                a[i]=a[j];
                a[j]=b;
            }
            
        }
    }
    printf("The sorted string is %s",a);
    return 0;
}

//input-god
//output-dgo
