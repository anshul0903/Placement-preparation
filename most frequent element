#include<bits/stdc++.h>
using namespace std;

int display(int arr[],int n)
{
    unordered_map<int,int> m;
    for(int i=0;i<n;i++)
    m[arr[i]]++;
    int max=0,res=-1;
    for(auto itr: m)
    {
        if(itr.second>max)
        {
            res=itr.first;
            max=itr.second;
        }
    }
    return res;
}



int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    cin>>arr[i];
    cout<<display(arr,n);
	
	return 0;
}
*******************************other use of iterartor********************
#include<bits/stdc++.h>
using namespace std;

int display(int arr[],int n)
{
    unordered_map<int,int> m;
    for(int i=0;i<n;i++)
    m[arr[i]]++;
    unordered_map<int,int> :: iterator itr;
    int max=0,res=-1;
    for(itr=m.begin();itr!=m.end();itr++)
    {
        if(itr->second>max)
        {
            res=itr->first;
            max=itr->second;
        }
    }
    return res;
}



int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    cin>>arr[i];
    cout<<display(arr,n);
	
	return 0;
}
I/p:
7
5 2 2 3 1 2 1
O/p:
2
