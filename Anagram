//input-recitals
//articles
//output-Strings are anagrams

#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    char a[10],b[10];
    scanf("%s",a);
    scanf("%s",b);
    int i,j,n,m,count=0;
    n=strlen(a);
    m=strlen(b);
    if(n==m)
    {
        for(i=0;i<n;i++)
        {
            for(j=0;j<m;j++)
            {
                if(a[i]==b[j])
                count++;
            }
        }
        if(count==n)
        printf("Strings are anagrams");
        else
        printf("Strings are not anagrams");
    }
        
        
        
	return 0;
}
