#include <stdio.h>

int main()
{
    int i, j, n,sum;
    scanf("%d", &n);
    for(i=1;i<=n;i++)
    {
        sum=1;
        for(j=1;j<=((2*i)-1);j++)
        {
            printf("%d",sum);
            if(j<i)
            {
            sum=sum+2;
            }
            else if(j>=i)
            {
                sum=sum-2;
            }
        }
        printf("\n");
    }

    return 0;
}

input 3
output:
1
131
13531
