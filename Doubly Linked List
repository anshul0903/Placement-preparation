#include <stdio.h>
#include <malloc.h>
#include <stdlib.h>
// doubly Linked list ;
struct node
{
    struct node *prev;
    int data;
    struct node *next;
};
struct node *start=NULL;
void create(int n);
void display();
int main()
{
    int t,n;
    scanf("%d",&t);
    while(t--)
    {
        scanf("%d",&n);
        create(n);
    }
    display();

	return 0;
}

void create(int n)
{
    struct node *ptr,*new_node,*preptr;
    new_node=(struct node*)malloc(sizeof(struct node));
    new_node->data=n;
    if(start==NULL)
    {
        new_node->prev=NULL;
        new_node->next=NULL;
        start=new_node;
    }
    else
    {
        ptr=start;
        while(ptr->next!=NULL)
        {
            ptr=ptr->next;
        }
        ptr->next=new_node;
        new_node->prev=ptr;
        new_node->next=NULL;
    }
}
void display()
{
    struct node *ptr;
    ptr=start;
    while(ptr!=NULL)
    {
        printf("%d->",ptr->data);
        ptr=ptr->next;
    }
}
